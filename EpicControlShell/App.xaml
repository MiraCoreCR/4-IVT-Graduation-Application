<Application x:Class="EpicControlShell.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:EpicControlShell"
             StartupUri="Windows/Auth/AuthorizationWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Icons.xaml"/>
            </ResourceDictionary.MergedDictionaries>

            <FontFamily x:Key="UbuntuMono">/EpicControlShell;component/Resources/Fonts/#Ubuntu Mono</FontFamily>
        
        </ResourceDictionary>
    </Application.Resources>
</Application>
